	"UbicationsGrid":{
		"TraderName":{
			"XPATH":".//zl-username/a",
			"Attribute":"Title"
		},
		"Url":{
			"XPATH":".//zl-username/a",
			"Attribute":"href"
		},
		"Pips":{
			"XPATH":".//tr/td/div[contains(@title,'pips ganados por el proveedor')]"
		},
		"Operaciones":{
			"XPATH":".//tr/td/div[contains(@title,'total de operaciones cerradas')]"
		},
		"Roi":{
			"XPATH":".//tr/td/div[contains(@title,'dinero ganado o perdido')]"
		},
		"MediaPips":{
			"XPATH":".//tr/td/div[contains(@title,'promedio de pips ganados o perdidos por')]"
		},
		"GananciaPorc":{
			"XPATH":".//tr/td/div[contains(@title,'El porcentaje de operaciones ganadoras')]"
		},
		"MaxDDPips":{
			"XPATH":".//tr/td/div[contains(@title,'La diferencia en pips entre el punto')]"
		},
		"Semanas":{
			"XPATH":".//tr/td/div[contains(@title,'de semanas que el proveedor')]"
		},
		"Usuarios":{
			"XPATH":".//tr/td/div[contains(@title,'de usuarios reales y demo que')]"
		},
		"CantidadSeguida":{
			"XPATH":".//tr/td/div[contains(@title,'saldo total de cuentas REALES siguiendo')]"
		},
		"BeneficioSegCuentaReal":{
			"XPATH":".//tr/td/div[contains(@title,'Este es el beneficio neto obtenido en las cuentas Reales')]"
		}
	},
	"UbicationsBadges":{
		"BadgeDollarGreen":{
			"ICON":"icon-badge-dollar-green"
		},
		"BadgeDollarBlue":{
			"ICON":"icon-badge-dollar-blue"
		},
		"BadgeFullyVerified":{
			"ICON":"icon-badge-fully-verified"
		},
		"BadgePartiallyVerified":{
			"ICON":"icon-badge-partially-verified"
		},
		"BadgeZuluscript":{
			"ICON":"icon-badge-zuluscript"
		},
		"BadgeApi":{
			"ICON":"icon-badge-api"
		},
		"BadgeFifo":{
			"ICON":"icon-badge-fifo"
		},
		"BadgeStar":{
			"ICON":"icon-badge-star"
		},
		"BadgeScale":{
			"ICON":"icon-badge-scale"
		},
		"BadgeVeteran":{
			"ICON":"icon-veteran"
		},
		"BadgeCalendar":{
			"ICON":"icon-badge-calendar"
		},
		"BadgeEA":{
			"ICON":"icon-badge-ea"
		}
	},
	"UbicationsInside":{
		"Order":{
			"XPATH":"//zl-trader-rank/div/span/strong",
			"Boolean":0
		},
		"ActividadRiesgosa":{
			"XPATH":"//zl-trader-warning/div[contains(@title,'Este Trader ha demostrado que el comportamiento de riesgo')]",
			"Boolean":1
		},
		"DeslizamientoSeguidoresAlto":{
			"XPATH":"//zl-trader-warning/div[contains(@title,'Slippage alto detectado')]",
			"Boolean":1
